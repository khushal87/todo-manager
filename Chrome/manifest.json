{
    "manifest_version": 2,
  
    "name": "Todo Manager",
    "description": "This helps you manage your day to day Todos",
    "version": "1.0",

    "icons": {
      "16": "favicon.png",
        "128": "favicon.png"
    },
  
    "browser_action": {
     "default_icon": "favicon.png",
     "default_popup": "index.html"
    },
    "permissions": ["tabs", "<all_urls>"]
  }